    "theme": "cerulean",
    "linenums": true,
    "collapseSymbols": false,
    "//analytics": {"ua":"UA-XXXXX-XXX", "domain":"XXXX"},
    "inverseNav": true,
    "highlightTutorialCode": true,
    "syntaxTheme": "default",
    "protocol": "html://"
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
